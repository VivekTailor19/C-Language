//Write C program to check whether a string is palindrome or not
#include<stdio.h>
#include<string.h>
main()
{
	char a[100],b[100];
	int n,cou;
	printf("Enter the String ::");
	gets(a);
	
	strcpy(b,a);
	strrev(a);              
	
	printf("\nB  is == ");
	puts(b);
	
	n = strcmp(a,b);              
	printf("n==== %d",n);
	if(n==0)
	{
		printf("\nIt's Palindrome");
	}
	else
	{
		printf("\n====It's  NOT Palindrome====");
	}
}
